@model IEnumerable<scoremaster_Presentation.Models.RubricCreate>
<div class="container-fluid">

    <!-- /# row -->
    <section id="main-content">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="bootstrap-data-table-panel">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered materialize-textarea">
                                <thead >
                                    <tr >
                                        <th style="background:blue;color:white;">Rubric Name</th>
                                        <th style ="background:blue;color:white;">Date</th>
                                        @* <th>Status</th> *@
                                        <th style="background:blue;color:white;"></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.RubricName </td>
                                            <td>@item.Date </td>
                                          @*   <td>@item.Isactive </td> *@
                                            <td style="color:white"><a role="button" class="btn btn-primary btn-rounded m-b-10 m-l-5" asp-action="RubricsLearingOutcome" asp-controller="Rubrics" asp-route-id="@item.RubricCreateId">Add PLo</a></td>
                                         
                                        </tr>
                                    }


                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <!-- /# card -->
            </div>
            <!-- /# column -->
        </div>
        <!-- /# row -->

    </section>
</div>