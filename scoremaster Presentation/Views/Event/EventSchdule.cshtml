
<div class="col-lg-12">
    <div class="card">
        <div class="card-title">
            <h3 style="text-align:center;">FYP Presentations Schedule</h3>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered">
                    <thead style="background:#373C58; ">
                        <tr>
                            <th style="color:white; font-family:'Times New Roman'; font-size:18px; text-align:center;">S/No</th>
                            <th style="color:white; font-family:'Times New Roman'; font-size:18px; text-align:center;">Project Title</th>
                            <th style="color:white; font-family:'Times New Roman'; font-size:18px; text-align:center;">Group Members</th>
                            <th style="color:white; font-family:'Times New Roman'; font-size:18px; text-align:center;">Panel Members</th>
                            <th style="color:white; font-family:'Times New Roman'; font-size:18px; text-align:center;">Presentation Schedule</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            int i = 1;
                        }
                        @foreach (var schdule in ViewBag.schdule)
                        {
                            <tr>
                                <th scope="row" style="font-family:'Times New Roman'; text-align:center;">@i</th>
                                <td style="font-family:'Times New Roman'; text-align:center;">@schdule.Title</td>
                                <td style="font-family:'Times New Roman'; text-align:center;">
                                    @foreach (var member in schdule.Members)
                                    {
                                        <p>@member.MemberName</p>
                                    }
                                </td>
                                <td style="font-family:'Times New Roman'; text-align:center;">
                                    @schdule.Supervisor <br />
                                    @schdule.CoSupervisor <br />
                                    <p style=" font-weight:bold;">External</p>
                                    @foreach (var examiner in schdule.ExternalExaminers)
                                    {
                                        <p>@examiner</p>
                                    }
                                </td>
                                <td class="color-primary" style="font-family:'Times New Roman'; text-align:center;">@schdule.EventDate <br />@schdule.EvenSchduled</td>
                            </tr>
                            i++;
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


