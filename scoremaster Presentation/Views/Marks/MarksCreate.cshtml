@model scoremaster_Presentation.Models.Marks
<div class="card card border-dark">
    <div class="card-header btn btn-primary border border-warning rounded">
        Mark Submission
    </div>
    <div class="card-body">
        <div class="container justify-content-center">
            <div class="container text-center">
                <div class="card mx-auto" style="width: 18rem;">
                    <img src="~/Img/rubrics.png" class="card-img-top" />
                    <div class="card-body">
                    </div>
                </div>
            </div>

            <form asp-action="MarksCreate" method="post">
                <input type="number"  hidden class="form-control"  asp-for="MemberDataId">
                <div class="form-row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="exampleInputPassword1">Marks</label>
                            <input type="text" class="form-control" asp-for="TotalMarks" required placeholder="Enter marks">
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <button type="submit" class="btn btn-primary form-control" style="margin-top:31px;">Save</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

