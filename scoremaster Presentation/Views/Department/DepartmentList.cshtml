@model IEnumerable<scoremaster_Presentation.Models.Department>
<div class="container-fluid">
   
    <!-- /# row -->
    <section id="main-content">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="bootstrap-data-table-panel">
                        <div class="table-responsive">
                            <table  class="table table-striped table-bordered materialize-textarea">
                                <thead>
                                    <tr>
                                        <th>Name</th>
                                        <th></th>
                                        @* <th>Office</th>
                                        <th>Salary</th> *@
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach(var item in Model)
                                    {
                                        <tr>
                                            <td>@item.DeptName </td>
                                            <td style="color:white"><a role="button" class="btn btn-primary btn-rounded m-b-10 m-l-5">Update</a></td>
                                            @* <td>Edinburgh</td>
                                            <td>$320,800</td> *@
                                        </tr>
                                    }
                                    
                                    
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <!-- /# card -->
            </div>
            <!-- /# column -->
        </div>
        <!-- /# row -->

    </section>
</div>